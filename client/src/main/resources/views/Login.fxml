<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.LoginView">
   <children>
      <Label layoutX="15.0" layoutY="46.0" text="Nom d'utilisateur :" />
      <Label layoutX="177.0" layoutY="104.0" text="Server" underline="true" />
      <Label layoutX="14.0" layoutY="148.0" text="Adresse IP du serveur : " />
      <Label layoutX="14.0" layoutY="191.0" text="Port de connexion : " />
      <TextField fx:id="txtUsername" layoutX="166.0" layoutY="41.0" promptText="Nom d'utilisateur" text="Ryadh" />
      <TextField fx:id="txtIPAddress" layoutX="166.0" layoutY="143.0" promptText="Adresse IP" text="192.168.1.26" />
      <TextField fx:id="txtPort" layoutX="166.0" layoutY="186.0" promptText="Port" text="6666" />
      <Button fx:id="btnConnect" layoutX="145.0" layoutY="255.0" mnemonicParsing="false" onAction="#connect" text="Se connecter" />
   </children>
</AnchorPane>
